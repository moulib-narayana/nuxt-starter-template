{
    "name": "nuxt-app",
    "private": true,
    "scripts": {
        "build": "source $HOME/.nvm/nvm.sh && nvm use; nuxt build",
        "dev": "source $HOME/.nvm/nvm.sh && nvm use; nuxt dev",
        "generate": "source $HOME/.nvm/nvm.sh && nvm use; nuxt generate",
        "preview": "source $HOME/.nvm/nvm.sh && nvm use; nuxt preview",
        "postinstall": "source $HOME/.nvm/nvm.sh && nvm use; nuxt prepare",
        "clean": "rm -rf .nuxt dist .output yarn.lock",
        "update": "yarn upgrade-interactive --latest"
    },
    "devDependencies": {
        "@element-plus/icons-vue": "^2.1.0",
        "@fingerprintjs/fingerprintjs": "^3.4.0",
        "@nuxt-alt/auth": "^2.5.0",
        "@nuxt-alt/http": "^1.6.1",
        "@pinia/nuxt": "^0.4.9",
        "@types/node": "^20.1.0",
        "@vueuse/core": "^10.1.0",
        "@vueuse/nuxt": "^10.1.0",
        "element-plus": "^2.3.4",
        "nuxt": "3.4.3",
        "sass": "^1.62.1",
        "vue-tsc": "^1.6.4"
    }
}
